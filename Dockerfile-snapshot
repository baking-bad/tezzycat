FROM cirrusci/wget

RUN wget --load-cookies /tmp/cookies.txt "https://docs.google.com/uc?export=download&confirm=$(wget --quiet --save-cookies /tmp/cookies.txt --keep-session-cookies --no-check-certificate 'https://docs.google.com/uc?export=download&id=1lqGnkLnPt41FibQ6CsBUdcUfwO_5QGIW' -O- | sed -rn 's/.*confirm=([0-9A-Za-z_]+).*/\1\n/p')&id=1lqGnkLnPt41FibQ6CsBUdcUfwO_5QGIW" -O tzkt_db.backup && rm -rf /tmp/cookies.txt
